{
  "ColA" :
    if (.ColA<10)
      1
    else
      if (.ColA>10 and .ColA<20)
       2
      else
       3
  ,

  "ColB" :
    if (.ColB>=50)
      2
    else
      1
  ,

  "ColC" :
    if (.ColC<50)
      2
    else
      1
  ,

  "ColD" :
    if (.ColD>=50)
      3
    else
      1
  ,

  * : .
}
